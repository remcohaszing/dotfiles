{
  "cloudcode.minikubeStatusBar": false,
  "coverage-gutters.showGutterCoverage": true,
  "coverage-gutters.showLineCoverage": false,
  "coverage-gutters.showRulerCoverage": true,
  "codesnap.shutterAction": "copy",
  "color-highlight.markerType": "dot-before",
  "color-highlight.markRuler": false,
  "color-highlight.languages": [
    "*",
    "!css",
    "!html",
    "!svg"
  ],
  "css.validate": false,
  "csv-preview.capitalizeHeaders": false,
  "debug.console.fontSize": 12,
  "debug.console.lineHeight": 16,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.foldingHighlight": false,
  "editor.fontFamily": "monospace",
  "editor.fontSize": 11,
  "editor.formatOnSave": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.guides.bracketPairs": true,
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": "on",
  "editor.renderWhitespace": "trailing",
  "editor.snippetSuggestions": "none",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "invalid.rainbow10",
        "settings": {
          "foreground": "#ffff88"
        }
      },
      {
        "scope": "entity.name.type",
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.unicodeHighlight.allowedCharacters": {
    "‘": true,
    "’": true,
    "“": true,
    "”": true,
    "–": true,
    "—": true
  },
  "editor.wordBasedSuggestions": "off",
  "editor.suggest.showWords": false,
  "emmet.excludeLanguages": [
    "typescriptreact"
  ],
  "emojisense.languages": {
    "git-commit": true,
    "html": true,
    "javascript": true,
    "javascriptreact": true,
    "markdown": true,
    "mdx": true,
    "plaintext": true,
    "python": true,
    "typescript": true,
    "typescriptreact": true,
    "yaml": true
  },
  "emojisense.unicodeCompletionsEnabled": true,
  "esbonio.server.enabled": false,
  "eslint.codeActionsOnSave.rules": [
    "!no-debugger",
    "!jest/prefer-todo",
    "!prettier/prettier",
    "*"
  ],
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "downgrade"
    },
    {
      "rule": "max-len",
      "severity": "info"
    },
    {
      "rule": "prettier/prettier",
      "severity": "info"
    },
    {
      "rule": "jest/expect-expect",
      "severity": "info"
    }
  ],
  "eslint.format.enable": true,
  "eslint.runtime": "/usr/bin/node",
  "explorer.autoRevealExclude": {},
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.desktop": "ini",
    ".env": "dotenv"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.factorypath": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.project": true,
    "**/.settings": true,
    "**/__pycache__": true,
    "**/CVS": true
  },
  "search.exclude": {
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/*.code-search": true,
    "**/.git": true,
    "*.map": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.enableStatusBarSync": false,
  "git.showCommitInput": false,
  "git.openRepositoryInParentFolders": "never",
  "gitlab.aiAssistedCodeSuggestions.enabled": false,
  "gitlab.showProjectMergeRequests": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "jest.autoRun": "off",
  "json.schemaDownload.enable": true,
  "keyboard.dispatch": "keyCode",
  "markdown.extension.tableFormatter.enabled": false,
  "markdown.extension.toc.updateOnSave": false,
  "markdown.extension.toc.levels": "2..6",
  "markdown.extension.toc.omittedFromToc": {
    "README.md": [
      "## Table of Contents"
    ]
  },
  "merge-conflict.diffViewPosition": "Beside",
  "prettier.requireConfig": true,
  "python.venvPath": "~/.local/share/virtualenvs",
  "remark.requireConfig": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "stylelint.enable": true,
  "stylelint.reportNeedlessDisables": true,
  "stylelint.validate": [
    "css",
    "less",
    "sass",
    "scss"
  ],
  "svg.preview.background": "transparent",
  "svg.preview.mode": "svg",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontSize": 12,
  "typescript.autoClosingTags": false,
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib",
  "update.mode": "none",
  "vsicons.dontShowNewVersionMessage": true,
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "hidden",
  "workbench.colorCustomizations": {
    "editor.background": "#000000",
    "editor.lineHighlightBackground": "#000000",
    "editorGutter.background": "#050505",
    "editorWarning.foreground": "#ffb055",
    "gitDecoration.ignoredResourceForeground": "#666666",
    "panel.background": "#000000",
    "panel.border": "#222222",
    "sideBar.background": "#050505",
    "statusBar.background": "#080808"
  },
  "workbench.colorTheme": "Blakai Theme",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.tabActionCloseVisibility": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editorAssociations": {
    "*.ico": "imagePreview.previewEditor",
    "*.png": "imagePreview.previewEditor"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.settings.editor": "json",
  "xo.enable": false,
  "xo.format.enable": false,
  "xo.overrideSeverity": "warn",
  "xo.runtime": "/usr/bin/node",
  "yaml.format.enable": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "unifiedjs.vscode-remark",
    "editor.formatOnSave": true
  },
  "[mdx]": {
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.rulers": [
      72,
      80
    ]
  },
  "[restructuredtext]": {
    "editor.wordWrap": "bounded"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
