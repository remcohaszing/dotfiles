{
  "breadcrumbs.enabled": false,
  "cloudcode.minikubeStatusBar": false,
  "css.validate": false,
  "csv-preview.capitalizeHeaders": false,
  "debug.console.fontSize": 10,
  "debug.console.lineHeight": 13,
  "debug.onTaskErrors": "debugAnyway",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.foldingHighlight": false,
  "editor.fontFamily": "monospace",
  "editor.fontSize": 11,
  "editor.formatOnSave": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": true,
  "editor.renderWhitespace": "trailing",
  "editor.snippetSuggestions": "none",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.underline.link",
        "settings": {
          "foreground": "#55bbff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "invalid.rainbow10",
        "settings": {
          "foreground": "#ffff88"
        }
      },
      {
        "scope": "markup.fenced_code.block.markdown",
        "settings": {
          "foreground": "#cfebea"
        }
      }
    ]
  },
  "emmet.excludeLanguages": [
    "typescriptreact"
  ],
  "emojisense.languages": {
    "git-commit": true,
    "javascript": true,
    "javascriptreact": true,
    "markdown": true,
    "plaintext": true,
    "python": true,
    "typescript": true,
    "typescriptreact": true,
    "yaml": true
  },
  "emojisense.unicodeCompletionsEnabled": true,
  "eslint.options": {
    "rules": {
      "no-debugger": "off"
    }
  },
  "eslint.format.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.desktop": "ini",
    "*.mmd": "Mermaid"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.factorypath": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.project": true,
    "**/.settings": true,
    "**/__pycache__": true,
    "**/CVS": true
  },
  "search.exclude": {
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/*.code-search": true,
    "**/.git": true,
    "*.map": true
  },
  "files.trimFinalNewlines": true,
  "gitlab.showIssueLinkOnStatusBar": false,
  "gitlab.showMrStatusOnStatusBar": false,
  "gitlab.showStatusBarLinks": false,
  "gitlab.showProjectMergeRequests": false,
  "html.format.enable": false,
  "javascript.autoClosingTags": false,
  "javascript.format.enable": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "javascript.validate.enable": false,
  "json.format.enable": false,
  "json.schemaDownload.enable": true,
  "keyboard.dispatch": "keyCode",
  "markdown.extension.tableFormatter.enabled": false,
  "markdown.extension.toc.updateOnSave": false,
  "nginx-conf-hint.syntax": "sublime",
  "prettier.requireConfig": true,
  "python.linting.pylintEnabled": false,
  "python.venvPath": "~/.local/share/virtualenvs",
  "security.workspace.trust.untrustedFiles": "open",
  "shellcheck.disableVersionCheck": true,
  "shellcheck.ignorePatterns": {
    ".zshrc": true,
    "**/*.zsh": true
  },
  "stylelint.reportNeedlessDisables": true,
  "yaml.maxItemsComputed": 1000,
  "stylelint.validate": ["css", "less", "sass", "scss"],
  "svg.preview.background": "transparent",
  "terminal.integrated.fontSize": 12,
  "typescript.autoClosingTags": false,
  "typescript.format.enable": false,
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsdk": "node_modules/typescript/lib",
  "update.mode": "none",
  "vsicons.dontShowNewVersionMessage": true,
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "editor.background": "#000000",
    "editor.lineHighlightBackground": "#000000",
    "editorGutter.background": "#050505",
    "editorWarning.foreground": "#ffb055",
    "gitDecoration.ignoredResourceForeground": "#666666",
    "panel.background": "#000000",
    "panel.border": "#222222",
    "sideBar.background": "#050505",
    "statusBar.background": "#080808"
  },
  "workbench.colorTheme": "Blakai Theme",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editorAssociations": {
    "*.ico": "imagePreview.previewEditor",
    "*.png": "imagePreview.previewEditor",
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.settings.editor": "json",
  "yaml.format.enable": false,

  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.rulers": [72, 80]
  },
  "[restructuredtext]": {
    "editor.wordWrap": "bounded"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
