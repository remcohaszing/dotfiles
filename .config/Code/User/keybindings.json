[
  {
    "key": "'",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "'"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+'",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "\""
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "`",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "`"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+9",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "("
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "[",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "["
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+[",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "{"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "F2",
    "command": "fileutils.renameFile",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+C",
    "command": "editor.action.clipboardCopyAction"
  },
  {
    "key": "ctrl+shift+right",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus"
  }
]
